/*我的资料顶部*/
.page_big_bg{
  width: 100%;
  height: 700rpx; 
}
.page_top{
   display:flex; 
   justify-content:center;
}
.img_icon {
  position: absolute;
  top:150rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  overflow: hidden;
  background: #00CC33;
}
.icon{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  overflow: hidden;
  position: relative;
  margin: 20rpx 25rpx 10rpx 25rpx;  
}
.number{
  position: absolute;
  top:380rpx;
  color:#fff;
  font-size:36rpx; 
}
.date{
  position: absolute;
  top:450rpx;
  color:#fff;
  font-size:32rpx; 
}
/*资料详情*/
.detail_panel{
  display:flex; 
  justify-content:center;
}
.page_detail{
  width:90%;
  height:500rpx;
  position: absolute;
  top:550rpx;
  background: #fff; 
  border-radius:20rpx; 
}